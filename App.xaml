<Application x:Class="Alienlab.DMT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dmt="clr-namespace:Alienlab.DMT">
  <Application.Resources>
    <dmt:AttachDatabaseViewModel x:Key="AttachDatabaseViewModel">
      <dmt:AttachDatabaseViewModel.DataSourceViewModel>
        <dmt:DataSourceViewModel/>
      </dmt:AttachDatabaseViewModel.DataSourceViewModel>
      <dmt:AttachDatabaseViewModel.DoAttach>
        <dmt:AttachDatabaseCommand/>
      </dmt:AttachDatabaseViewModel.DoAttach>
    </dmt:AttachDatabaseViewModel>
    <dmt:DetachDatabaseViewModel x:Key="DetachDatabaseViewModel">
      <dmt:DetachDatabaseViewModel.DataSourceViewModel>
        <dmt:DataSourceViewModel/>
      </dmt:DetachDatabaseViewModel.DataSourceViewModel>
      <dmt:DetachDatabaseViewModel.DoDetach>
        <dmt:DetachDatabaseCommand/>
      </dmt:DetachDatabaseViewModel.DoDetach>
    </dmt:DetachDatabaseViewModel>
  </Application.Resources>
</Application>
